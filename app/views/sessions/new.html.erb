<%= flash.notice %>

<%= form_with(scope: :session, url: login_path, local: true) do |form| %> 
  <%= form.text_field :username, type: "string" , placeholder:"Username"%>
  <%= form.password_field :password, type: "string", placeholder:"Password"%>
  <div>
    <%= form.submit "Log In" , class: "btn btn-primary"%>
  </div>
<%end%>
