<h1>Your Posts</h1>
<%= link_to "New post", new_post_path %>
<div id="posts">
  <% @posts.each do |post| %>
    <%= render post %>
    <% if post.user_id == current_user.id %>  
      <%= button_to "Destroy this post", post, method: :delete %>
    <% end %> 
    
    <p>
      <%= link_to "Show this post", post %>
    </p>
  <% end %>
</div>